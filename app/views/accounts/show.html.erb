<h1>Accounts#show</h1>
<div>
  <h1><%= @account.full_name %></h1>
  <p>Account number: <%= @account.account_number %></p>
  <% if @account.address %>
  <p>Address: <%= @account.address %>, <%= @account.suburb %></p>, <%= @account.postcode %></p>
  <p>Mobile number: <%= @account.mobile %></p>
  <p>Social media: <%= @account.social_media %></p>
  <% end %>
</div>
<%= link_to "Update Account", edit_account_path %>
<%= link_to "Back", root_path %>